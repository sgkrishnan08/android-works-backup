<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.button">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".scrollentrybase"></activity>
        <activity android:name=".scrollentry"
            android:theme="@style/AppTheme.CustomTheme">
        </activity>

        <activity android:name=".sample" />
        <activity android:name=".companydetails" />
        <activity android:name=".profile" />
        <activity android:name=".entrypage" />
        <activity android:name=".customeraddition" />
        <activity android:name=".emptybutton" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>