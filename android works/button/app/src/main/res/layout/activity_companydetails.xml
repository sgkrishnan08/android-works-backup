<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".companydetails">

    <Button
        android:id="@+id/companydetailsclear"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:text="clear"
        app:layout_constraintStart_toEndOf="@+id/companydetailssave"
        app:layout_constraintTop_toTopOf="@+id/companydetailssave" />

    <Button
        android:id="@+id/companydetailsset"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="206dp"
        android:layout_marginRight="206dp"
        android:text="set"
        app:layout_constraintBaseline_toBaselineOf="@+id/companydetailssave"
        app:layout_constraintEnd_toStartOf="@+id/sampleexit"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/companydetailssave"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="23dp"
        android:text="Save"
        app:layout_constraintStart_toEndOf="@+id/companydetailsset"
        app:layout_constraintTop_toBottomOf="@+id/companydetailsdisplay" />

    <Button
        android:id="@+id/sampleexit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:text="Exit"
        app:layout_constraintBaseline_toBaselineOf="@+id/companydetailsclear"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/companydetailsset" />

    <EditText
        android:id="@+id/companydetailsownername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="@+id/companydetailscustomername"
        app:layout_constraintTop_toBottomOf="@+id/companydetailscustomername" />

    <EditText
        android:id="@+id/companydetailscustomeraddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintStart_toStartOf="@+id/companydetailsownername"
        app:layout_constraintTop_toBottomOf="@+id/companydetailsownername" />

    <EditText
        android:id="@+id/companydetailscustomerp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:ems="10"
        android:hint="Optional"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/companydetailscustomerp1"
        app:layout_constraintTop_toBottomOf="@+id/companydetailscustomerp1" />

    <EditText
        android:id="@+id/companydetailscustomername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/companydetailscustomerarea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:ems="10"
        android:hint="Enter your area/taluk"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="@+id/companydetailscustomeraddress"
        app:layout_constraintTop_toBottomOf="@+id/companydetailscustomeraddress" />

    <EditText
        android:id="@+id/companydetailscustomerp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:hint=""
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/companydetailscustomerarea"
        app:layout_constraintTop_toBottomOf="@+id/companydetailscustomerarea" />

    <ImageButton
        android:id="@+id/companydetailshomelogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_spa_black_24dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Mobile number"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/textView15" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="27dp"
        android:layout_height="0dp"

        android:layout_marginTop="42dp"
        android:layout_marginBottom="42dp"
        app:layout_constraintBottom_toTopOf="@+id/companydetailsdisplay"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/companydetailscustomerp1"
        app:srcCompat="@drawable/ic_local_phone_black_24dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="108dp"
        android:layout_marginBottom="109dp"
        android:text="company name"
        app:layout_constraintBottom_toBottomOf="@+id/companydetailscustomeraddress"
        app:layout_constraintStart_toStartOf="@+id/companydetailshomelogo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Address"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="owner Name"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/companydetailsdisplay"
        android:layout_width="374dp"
        android:layout_height="217dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="112dp"
        android:fontFamily="sans-serif-condensed"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="411dp" />


</androidx.constraintlayout.widget.ConstraintLayout>